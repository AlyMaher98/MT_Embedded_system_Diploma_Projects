#include "../HAL/keypad.h"
#include "../HAL/lcd.h"
#include "../MCAL/gpio.h"
#include "../MCAL/uart.h"

void LCD_WritePass(void)
{
	LCD_clearScreen();
	LCD_displayString("Enter Password:")
	LCD_moveCursor(1,0);
	}
void LCD_ReWritePass(void)
{
	LCD_clearScreen();
	LCD_displayString("Re-Enter Pass:")
	LCD_moveCursor(1,0);
	}
void LCD_Astricsprint(void)
{
	LCD_displayString("*")

}

void LCD_MainMenu(void)
{
	LCD_clearScreen();
	LCD_displayString("Re-Enter Pass:")
	LCD_moveCursor(1,0);

	}
int main()
{
	LCD_init();


while (1)
{

}
}
